{
  "name": "webpush-server",
  "version": "0.9.0",
  "description": "Webpush Notification server",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx",
    "start": "node -r dotenv/config src/server",
    "generate-keys": "node_modules/web-push/src/cli.js generate-vapid-keys"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmagaz/webpush-server.git"
  },
  "keywords": [
    "webpush"
  ],
  "author": "Pablo Magaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmagaz/webpush-server/issues"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "commit-msg": "echo $HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/pmagaz/webpush-server#readme",
  "dependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-transform-runtime": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/register": "7.4.0",
    "@babel/runtime": "7.4.2",
    "babel-plugin-module-resolver": "3.2.0",
    "body-parser": "1.18.3",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "husky": "1.3.1",
    "nedb": "1.8.0",
    "web-push": "3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0"
  }
}
